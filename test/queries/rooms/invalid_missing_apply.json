{
    "title": "transformations missing apply",
    "query": {
      "WHERE": {
        "AND": [
          {
            "IS": {
              "rooms_furniture": "*Tables*"
            }
          },
          {
            "GT": {
              "rooms_seats": 300
            }
          }
        ]
      },
      "OPTIONS": {
        "COLUMNS": [
          "rooms_shortname",
          "max Seats"
        ],
        "ORDER": {
          "dir": "UP",
          "keys": [
            "rooms_shortname",
            "max Seats"
          ]
        }
      },
      "TRANSFORMATIONS": {
        "GROUP": [
          "rooms_shortname"
        ]
      }
    },
    "isQueryValid": false,
    "result": "InsightError"
}
