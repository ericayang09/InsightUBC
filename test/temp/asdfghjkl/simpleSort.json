{
    "title": "Rooms simple sort with ORDER UP",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 300
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname"
            ],
            "ORDER": {
                "dir": "UP",
                "keys": [
                    "rooms_shortname"
                ]
            }
        }
    },
    "isQueryValid": true,
    "result": [
        {
            "rooms_shortname": "HEBB"
        },
        {
            "rooms_shortname": "LSC"
        },
        {
            "rooms_shortname": "LSC"
        },
        {
            "rooms_shortname": "OSBO"
        }
    ]
}
