{
    "title": "Simple Rooms query that does not use transformations",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 300
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "rooms_lon",
                "rooms_lat",
                "rooms_furniture"
            ]
        }
    },
