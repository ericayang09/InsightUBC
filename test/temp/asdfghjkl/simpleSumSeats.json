{
    "title": "Sum seats",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 300
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "countType"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "countType",
                    "rooms_shortname"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_shortname"
            ],
            "APPLY": [
                {
                    "countType": {
                        "SUM": "rooms_seats"
                    }
                }
            ]
        }
    },
    "isQueryValid": true,
    "result": [
        {
            "rooms_shortname": "LSC",
            "countType": 700
        },
        {
            "rooms_shortname": "OSBO",
            "countType": 442
        },
        {
            "rooms_shortname": "HEBB",
            "countType": 375
        }
    ]
}
