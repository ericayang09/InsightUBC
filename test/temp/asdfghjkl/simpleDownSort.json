{
    "title": "Simple sort on Rooms using dir DOWN",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 300
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "rooms_seats"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "rooms_seats"
                ]
            }
        }
    },
    "isQueryValid": true,
    "result": [
        {
            "rooms_shortname": "OSBO",
            "rooms_seats": 442
        },
        {
            "rooms_shortname": "HEBB",
            "rooms_seats": 375
        },
        {
            "rooms_shortname": "LSC",
            "rooms_seats": 350
        },
        {
            "rooms_shortname": "LSC",
            "rooms_seats": 350
        }
    ]
}
