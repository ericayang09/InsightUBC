{
    "title": "Transformations with valid group, apply, and average",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_id": "340"
                    }
                },
                {
                    "IS": {
                        "courses_dept": "cpsc"
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_id",
                "overallAvg"
            ]
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_id"
            ],
            "APPLY": [
                {
                    "overallAvg": {
                        "AVG": "courses_avg"
                    }
                }
            ]
        }
    },
    "isQueryValid": true,
    "result": [
        {
            "courses_id": "340",
            "overallAvg": 75.69
        }
    ]
}
