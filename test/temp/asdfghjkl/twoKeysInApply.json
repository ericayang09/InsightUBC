{
    "title": "Apply has two keys, should only have one",
    "query": {
  "WHERE": {
    "AND": [
      {
        "IS": {
          "rooms_furniture": "*Tables*"
        }
      },
      {
        "GT": {
          "rooms_seats": 100
        }
      }
    ]
  },
  "OPTIONS": {
    "COLUMNS": [
      "rooms_shortname",
      "maxSeats"
    ],
    "ORDER": {
      "dir": "DOWN",
      "keys": [
        "maxSeats",
        "countShortname"
      ]
    }
  },
  "TRANSFORMATIONS": {
    "GROUP": [
      "rooms_shortname"
    ],
    "APPLY": [
      {
        "maxSeats": {
          "MAX": "rooms_seats"
        },
        "countShortname": {
          "COUNT": "rooms_shortname"
        }
      }
    ]
  }
},
    "isQueryValid": false,
    "result": "InsightError"
}
